{
   "class" : "org.apache.solr.ltr.model.LinearModel",
   "store": "store1",
   "name" : "linearModel2",
   "features" : [
       { "name" : "titleLength" },
       { "name" : "originalScore" },
       { "name" : "downloads" },
			 { "name" : "isFavouriteSubject" },
			 { "name" : "userAge" },
			 { "name" : "isUserFromMobile" }
   ],
   "params" : {
       "weights" : {
           "titleLength" : 1000.0,
           "originalScore" : 0.5,
           "downloads" : 100.0,
					 "isFavouriteSubject" : 1.0,
					 "userAge" : 0.5,
					 "isUserFromMobile" : 2.0
       }
   }
}
